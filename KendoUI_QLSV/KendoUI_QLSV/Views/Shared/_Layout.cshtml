<!DOCTYPE html>
<html>
    <head>
        <meta charset="utf-8" />
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <title>@ViewBag.Title - My Kendo UI MVC Application</title>
    	<link href="@Url.Content("~/Content/kendo/2021.3.914/kendo.bootstrap-v4.min.css")" rel="stylesheet" type="text/css" />
	<link href="@Url.Content("~/Content/bootstrap.min.css")" rel="stylesheet" type="text/css" />
	<link href="@Url.Content("~/Content/Site.css")" rel="stylesheet" type="text/css" />
	<script src="@Url.Content("~/Scripts/kendo/2021.3.914/jquery.min.js")"></script>
	<script src="@Url.Content("~/Scripts/kendo/2021.3.914/angular.min.js")"></script>
	<script src="@Url.Content("~/Scripts/kendo/2021.3.914/jszip.min.js")"></script>
	<script src="@Url.Content("~/Scripts/kendo/2021.3.914/kendo.all.min.js")"></script>
	<script src="@Url.Content("~/Scripts/bootstrap.min.js")"></script>
</head>
    <body class="k-content">
        <nav class="navbar navbar-inverse navbar-fixed-top p-3">
            <div class="container p-0 p-sm-3">
                <div class="navbar-header">
                    <button id="configure" class="k-rpanel-toggle k-button k-primary btn-toggle"><span class="k-icon k-i-menu"></span></button>
                    @*@Html.ActionLink("your logo here", "Index", new { controller = "Home" }, new { @class = "navbar-brand" })*@
                    <a href="@Url.Action("Index", "Home")">
                        <img src="https://vnresource.vn/templates/skylab/images/logo.png" alt="" />
                    </a>
                </div>
                <div id="responsive-panel" class="navbar-left">
                    <ul id="menu">
                        <li>@Html.ActionLink("Trang chủ", "Index", "Home")</li>
                        <li>@Html.ActionLink("Thông tin thêm", "About", "Home")</li>
                        <li>@Html.ActionLink("Liên hệ", "Contact", "Home")</li>
                        <li>@Html.ActionLink("Danh Sách Sinh Viên", "getData", "SinhVien")</li>
                    </ul>
                </div>
            </div>
        </nav>
    
        <main>       
            <div class="container">
                @RenderSection("featured", required: false)
                @RenderBody()
            </div>
        </main>
    
        <footer class="footer text-center d-flex align-items-center">
            <div class="container pb-0">
                <hr />
                <p>Copyright © 2012 - 2021 VnResource Corp. VnResource.vn</p>
            </div>
        </footer>
    
        <script>
            $(document).ready(function () {
                $("#responsive-panel").kendoResponsivePanel({
                    breakpoint: 768,
                    autoClose: false,
                    orientation: "top"
                });
                $("#menu").kendoMenu();
            });
            function onclick(e) {
                $("#responsive-panel").getKendoResponsivePanel().toggle();
            }
        </script>
    </body>
</html>